<test-suite>
  <meta>
    <title>Basic Test Queries</title>
  </meta>
  <setup>
    <keys>
      <key id="doctype">name(/*)</key>
      <key id="title">/*/TITLE</key>
      <key id="title_all">//TITLE</key>
      <key id="scnnum" type="int">/*/@scene</key>
      <key id="actnum" type="int">/*/@act</key>
    </keys>
  </setup>
  <test-cases>
    <include file="base-cases.xml" />
  </test-cases>
  <queries id="qname-index">
    <include file="qname-queries.xml" />
  </queries>
</test-suite>
